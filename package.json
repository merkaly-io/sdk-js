  "lint-staged": {
    "*.{js,ts,vue}": "yarn run lint:fix"
  },
  "dependencies": {
    "@merkaly/api": "^0.1.6",
    "@nuxtjs/axios": "^5.13.6",
    "core-js": "^3.19.3",
